type Task = record {
  id : nat64;
  data : text;
  frequency : nat64;
  last_run : nat64;
};

service : {
  init : () -> ();
  create_task : (nat64, text, nat64) -> ();
  get_tasks : () -> (vec Task) query;
  delete_task : (nat64) -> ();
  execute_tasks : () -> ();
  retire_agent : () -> ();
  cycles_available : () -> (nat64) query;
}
